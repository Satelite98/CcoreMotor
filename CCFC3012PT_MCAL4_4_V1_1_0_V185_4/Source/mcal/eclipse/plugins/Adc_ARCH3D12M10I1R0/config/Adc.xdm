<?xml version='1.0'?>
<datamodel version="7.0" 
           xmlns="http://www.tresos.de/_projects/DataModel2/16/root.xsd" 
           xmlns:a="http://www.tresos.de/_projects/DataModel2/16/attribute.xsd" 
           xmlns:v="http://www.tresos.de/_projects/DataModel2/06/schema.xsd" 
           xmlns:d="http://www.tresos.de/_projects/DataModel2/06/data.xsd">
<!--
*   @file    Adc.xdm
*   @version 
*
*   @brief   AUTOSAR Adc - Tresos Studio plugin schema file
*   @details This file contains the schema configuration for and Adc Tresos Studio
*            plugin.
-->
  <d:ctr type="AUTOSAR" factory="autosar" 
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd" 
         xmlns:cd="http://www.tresos.de/_projects/DataModel2/08/customdata.xsd" 
         xmlns:f="http://www.tresos.de/_projects/DataModel2/14/formulaexpr.xsd" 
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd" 
         xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd"  
         xmlns:variant="http://www.tresos.de/_projects/DataModel2/11/variant.xsd">
    <d:lst type="TOP-LEVEL-PACKAGES">
      <d:ctr name="ARCH3D12M10I1R0" type="AR-PACKAGE">
        <d:lst type="ELEMENTS">
          <!--  /** @implements Adc */ -->
          <d:chc name="Adc" type="AR-ELEMENT" value="MODULE-DEF">
            <v:ctr type="MODULE-DEF">
              <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                <ad:ADMIN-DATA>
                  <ad:DOC-REVISIONS>
                    <ad:DOC-REVISION>
                      <ad:REVISION-LABEL>4.4.0</ad:REVISION-LABEL>
                      <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                    </ad:DOC-REVISION>
                  </ad:DOC-REVISIONS>
                </ad:ADMIN-DATA>
              </a:a>
              <a:a name="DESC" 
                   value="EN: Configuration of the Adc (Analog Digital Conversion) module."/>
              <a:a name="LOWER-MULTIPLICITY" value="1"/>
              <a:a name="POSTBUILDVARIANTSUPPORT" value="true"/>
              <a:a name="RELEASE" value="asc:4.4.0"/>
              <a:a name="UPPER-MULTIPLICITY" value="1"/>
              <a:a name="UUID" value="dcec3f6a-0d96-40a4-87db-bf285419c755"/>

              <!--  /** @implements  ConfigVariant */ -->
              <v:var name="POST_BUILD_VARIANT_USED" type="BOOLEAN">
                <a:a name="DESC" 
                     value="Indicates whether a module implementation has or plans to have (i.e., introduced at link or post-build time) new post-build variation points."/>
                <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                     type="IMPLEMENTATIONCONFIGCLASS">
                  <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                </a:a>
                <a:a name="LABEL" value="Post Build Variant Used"/>
                <a:a name="ORIGIN" value="EB"/>
                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                <a:a name="UUID" value="9a08292b-b817-4e0e-b6f7-deec8cc5e80e"/>
                <a:a name="__ORIGIN" value="Elektrobit"/>
                <a:da name="DEFAULT" value="false"/>
                <a:da name="READONLY" value="FALSE"/>
                <a:da name="TOOLTIP" 
                      value="Indicates whether a module implementation has or plans to have (i.e., introduced at link or post-build time) new post-build variation points."/>
              </v:var>
              <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                     type="IMPLEMENTATIONCONFIGCLASS">
                  <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                  <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                </a:a>
                <a:a name="LABEL" value="Config Variant"/>
                <a:a name="UUID" value="2a9c7afb-e93b-42cd-a178-9078d2bb15f8"/>
                <a:a name="__ORIGIN" value="Elektrobit"/>
                <a:da name="DEFAULT" value="VariantPostBuild"/>
                <a:da name="RANGE">
                  <a:v>VariantPostBuild</a:v>
                  <a:v>VariantPreCompile</a:v>
                </a:da>
              </v:var>

              <!--  @implements AdcConfigSet  -->
              <v:ctr name="AdcConfigSet" type="IDENTIFIABLE">
                <a:a name="DESC" 
                     value="EN: This container contains the configuration parameters and sub containers of the AUTOSAR Adc module."/>
                <a:a name="UUID" value="2c9b79ae-bb23-4257-8cf8-bc32dd1b8f16"/>

                <!--  @implements AdcHwUnit  -->
                <v:lst name="AdcHwUnit" type="MAP">
                  <a:da name="MIN" value="1"/>
                  <a:da name="MAX" value="13"/>

                  <!-- AdcHwUnit Big container -->
                  <v:ctr name="AdcHwUnit" type="IDENTIFIABLE">
                    <a:a name="DESC" 
                         value="EN: This container contains the Driver configuration (parameters) depending on grouping of channels"/>
                    <a:a name="UUID" 
                         value="0bb45f4e-c9ec-4fbc-9e8c-ad6389cb095b"/>

                    <!-- AdcClockSource -->
                    <v:var name="AdcClockSource" type="ENUMERATION">
                      <a:a name="DESC" 
                           value="EN: The ADC module specific clock input for the conversion unit can statically be configured to select different clock sources if provided by hardware. Enumeration literals are defined vendor specific."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" 
                           value="44749b8c-b3bd-420e-8526-e415ca17ffd8"/>
                      <a:a name="__ORIGIN" value="Elektrobit"/>
                      <a:da name="DEFAULT" value="EQADC_CLK_SEL_PS_DIV"/>
                      <a:da name="RANGE">
                            <a:v>EQADC_CLK_SEL_PS_DIV</a:v>
                      </a:da>
                    </v:var>

                    <!-- ADCHwUnitId -->
                    <v:var name="AdcHwUnitId" type="ENUMERATION">
                      <a:a name="DESC"> 
                          <a:v><![CDATA[EN:<html><p>
                              Specifies the used ADC Hardware Unit. This symbolic name allows accessing Hw Unit data.
                              <br><li>ADC0 and ADC1 are corresponding ADC number 0 and ADC number 1 of EQADC_01.</li></br>
                              <br><li>ADC2 and ADC3 are corresponding ADC number 0 and ADC number 1 of EQADC_23.</li></br>
                              <br><li>ADC4 and ADC5 are corresponding ADC number 0 and ADC number 1 of EQADC_45.</li></br>
                              <br><li>ADC6 and ADC7 are corresponding ADC number 0 and ADC number 1 of EQADC_67.</li></br>
                              <br><li>ADC8 and ADC9 are corresponding ADC number 0 and ADC number 1 of EQADC_89.</li></br>
                              <br><li>ADC10 and ADC11 are corresponding ADC number 0 and ADC number 1 of EQADC_1011.</li></br>
                              <br><li>ADC12 is corresponding ADC number 0 of EQADC_1213.</li></br>
                              </p></html>]]></a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" 
                           value="d67c9731-c389-4cea-92b4-5fb8046989e5"/>
                      <a:a name="__ORIGIN" value="Elektrobit"/>
                      <a:da name="DEFAULT" type="XPath" expr="concat('ADC', string(node:fallback(node:current()/../@index,'0')))"/>
                      <a:da name="INVALID" type="XPath">
                          <a:tst expr="text:uniq(node:fallback( ../../*/AdcHwUnitId, 0), node:fallback( ., 1))" false="Duplicate ADC Hardware ID"> </a:tst>
                      </a:da>
                      <a:da name="RANGE" type="XPath" expr="ecu:list('EQADC.EQADCHwUnitList')"/>
                    </v:var>

                    <!-- AdcPrescale -->
                    <v:var name="AdcPrescale" type="ENUMERATION">
                      <a:a name="DESC" 
                           value="EN: Optional ADC module specific clock prescale factor, if supported by hardware."/>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                           type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" 
                           value="450a00d8-d670-4fdc-a446-bd952d4505f8"/>
                      <a:a name="__ORIGIN" value="Elektrobit"/>
                      <a:da name="DEFAULT" value="CLK_SEL_PS_DIV_2"/>
                      <a:da name="RANGE">
                            <a:v>CLK_SEL_PS_DIV_2</a:v>
                            <a:v>CLK_SEL_PS_DIV_4</a:v>
                            <a:v>CLK_SEL_PS_DIV_6</a:v>
                            <a:v>CLK_SEL_PS_DIV_8</a:v>
                            <a:v>CLK_SEL_PS_DIV_10</a:v>
                            <a:v>CLK_SEL_PS_DIV_12</a:v>
                            <a:v>CLK_SEL_PS_DIV_14</a:v>
                            <a:v>CLK_SEL_PS_DIV_16</a:v>
                            <a:v>CLK_SEL_PS_DIV_18</a:v>
                            <a:v>CLK_SEL_PS_DIV_20</a:v>
                            <a:v>CLK_SEL_PS_DIV_22</a:v>
                            <a:v>CLK_SEL_PS_DIV_24</a:v>
                            <a:v>CLK_SEL_PS_DIV_26</a:v>
                            <a:v>CLK_SEL_PS_DIV_28</a:v>
                            <a:v>CLK_SEL_PS_DIV_30</a:v>
                            <a:v>CLK_SEL_PS_DIV_32</a:v>
                            <a:v>CLK_SEL_PS_DIV_34</a:v>
                            <a:v>CLK_SEL_PS_DIV_36</a:v>
                            <a:v>CLK_SEL_PS_DIV_38</a:v>
                            <a:v>CLK_SEL_PS_DIV_40</a:v>
                            <a:v>CLK_SEL_PS_DIV_42</a:v>
                            <a:v>CLK_SEL_PS_DIV_44</a:v>
                            <a:v>CLK_SEL_PS_DIV_46</a:v>
                            <a:v>CLK_SEL_PS_DIV_48</a:v>
                            <a:v>CLK_SEL_PS_DIV_50</a:v>
                            <a:v>CLK_SEL_PS_DIV_52</a:v>
                            <a:v>CLK_SEL_PS_DIV_54</a:v>
                            <a:v>CLK_SEL_PS_DIV_56</a:v>
                            <a:v>CLK_SEL_PS_DIV_58</a:v>
                            <a:v>CLK_SEL_PS_DIV_60</a:v>
                            <a:v>CLK_SEL_PS_DIV_62</a:v>
                            <a:v>CLK_SEL_PS_DIV_64</a:v>
                      </a:da>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr="num:i(node:ref(../../../../AdcGeneral/AdcClockRef)/McuClockReferencePointFrequency) 
                                div num:i(substring-after(string(.),'CLK_SEL_PS_DIV_')) > num:i(16000000)"
                                true="ADC module clock frequency should be less than or equal to 16MHz"/>
                      </a:da>
                    </v:var>

                    <!-- AdcExternalMux -->
                    <v:var name="AdcExternalMux" type="BOOLEAN">
                      <a:a name="DESC"> 
                            <a:v><![CDATA[EN:<html><p>Enables external multiplexing (ADCn_EMUX bit in ADCn_CR register). The ADC units of one EQADC (0-1) cannot have external multiplexing enabled at the same time. Note: This is an Implementation Specific Parameter.</p></html>]]></a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                            type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="CCFC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" 
                            value="cd1c125f-21e4-4c23-2574-6dd6a122164ede"/>
                      <a:a name="__ORIGIN" value="Elektrobit"/>
                      <a:da name="DEFAULT" value="false"/>
                      <a:da name="INVALID" type="XPath">
                        <a:tst expr="node:exists(../../*[AdcHwUnitId = 'ADC0']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC0']/AdcExternalMux = 'true') and node:exists(../../*[AdcHwUnitId = 'ADC1']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC1']/AdcExternalMux = 'true')"
                                true="This parameter can not be enabled for both for both ADC0 and ADC1"/>
                        <a:tst expr="node:exists(../../*[AdcHwUnitId = 'ADC2']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC2']/AdcExternalMux = 'true') and node:exists(../../*[AdcHwUnitId = 'ADC3']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC3']/AdcExternalMux = 'true')"
                                true="This parameter can not be enabled for both for both ADC2 and ADC3"/>
                        <a:tst expr="node:exists(../../*[AdcHwUnitId = 'ADC4']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC4']/AdcExternalMux = 'true') and node:exists(../../*[AdcHwUnitId = 'ADC5']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC5']/AdcExternalMux = 'true')"
                                true="This parameter can not be enabled for both for both ADC4 and ADC5"/>
                        <a:tst expr="node:exists(../../*[AdcHwUnitId = 'ADC6']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC6']/AdcExternalMux = 'true') and node:exists(../../*[AdcHwUnitId = 'ADC7']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC7']/AdcExternalMux = 'true')"
                                true="This parameter can not be enabled for both for both ADC6 and ADC7"/>
                        <a:tst expr="node:exists(../../*[AdcHwUnitId = 'ADC8']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC8']/AdcExternalMux = 'true') and node:exists(../../*[AdcHwUnitId = 'ADC9']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC9']/AdcExternalMux = 'true')"
                                true="This parameter can not be enabled for both for both ADC8 and ADC9"/>
                        <a:tst expr="node:exists(../../*[AdcHwUnitId = 'ADC10']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC10']/AdcExternalMux = 'true') and node:exists(../../*[AdcHwUnitId = 'ADC11']/AdcExternalMux) and (../../*[AdcHwUnitId = 'ADC11']/AdcExternalMux = 'true')"
                                true="This parameter can not be enabled for both for both ADC10 and ADC11"/>
                      </a:da>
                    </v:var>

                    <!-- AdcResolution -->
                    <v:var name="Resolution" type="ENUMERATION">
                      <a:a name="DESC">
                          <a:v><![CDATA[EN:<html><p>Used to set the ADC resolution mode.
                              <ul>
                              <li>ADC_RESOLUTION_BIT8    -- 8-bit conversion.</li>
                              <li>ADC_RESOLUTION_BIT10   -- 10-bit conversion.</li>
                              <li>ADC_RESOLUTION_BIT12   -- 12-bit conversion.</li>
                              </ul>.</p></html>]]></a:v>
                      </a:a>
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                            type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="CCFC"/>
                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                      <a:a name="SCOPE" value="LOCAL"/>
                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                      <a:a name="UUID" 
                            value="cd1c565f-85e4-4c23-2574-6dd6a6364ede"/>
                      <a:a name="__ORIGIN" value="Elektrobit"/>
                      <a:da name="DEFAULT" value="ADC_RESOLUTION_BIT12"/>
                      <a:da name="RANGE">
                            <a:v>ADC_RESOLUTION_BIT8</a:v>
                            <a:v>ADC_RESOLUTION_BIT10</a:v>
                            <a:v>ADC_RESOLUTION_BIT12</a:v>
                      </a:da>
                    </v:var>

                    <!-- AlternateConfig -->
                    <v:lst name="AlternateConfig" type="MAP">
                      <a:da name="MAX" value="1"/>
                      <a:da name="INVALID" type="XPath">
                      <a:tst
                          expr="../Resolution != 'ADC_RESOLUTION_BIT12' and num:i(count(./*)) = 0"
                          true="Please configure AlternateConfig containers."/>
                      </a:da>
                      <v:ctr name="AlternateConfig" type="IDENTIFIABLE">
                        <a:a name="DESC" value = "EN: Set alternative configuration."/>
                        <a:a name="ORIGIN" value="CCFC"/>
                        <a:a name="UUID" 
                              value="3cacb124-d0108-480e-9ae9-ad701abac080"/>
                        <a:a name="__ORIGIN" value="Elektrobit"/>
                        <a:a name="READONLY" value="FALSE"/>

                        <v:var name="AltSelect" type="ENUMERATION">
                          <a:a name="DESC"
                            value="EN: Select the Alternate Configuration x."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                              type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                              value="3cacb124-d0108-480e-9ae9-ad701abac081"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="EQADC_ALT_CONFIG_DISABLE"/>
                          <a:da name="RANGE">
                            <a:v>EQADC_ALT_CONFIG_DISABLE</a:v>
                            <a:v>EQADC_ALT_CONFIG_SEL_1</a:v>
                            <a:v>EQADC_ALT_CONFIG_SEL_2</a:v>
                            <a:v>EQADC_ALT_CONFIG_SEL_3</a:v>
                            <a:v>EQADC_ALT_CONFIG_SEL_4</a:v>
                            <a:v>EQADC_ALT_CONFIG_SEL_5</a:v>
                            <a:v>EQADC_ALT_CONFIG_SEL_6</a:v>
                            <a:v>EQADC_ALT_CONFIG_SEL_7</a:v>
                            <a:v>EQADC_ALT_CONFIG_SEL_8</a:v>
                          </a:da>
                          <a:da name="INVALID" type="XPath">
                          <a:tst
                              expr="(../../../Resolution != 'ADC_RESOLUTION_BIT12') and (. = 'EQADC_ALT_CONFIG_DISABLE')"
                              true="Please select another configuration."/>
                          </a:da>
                        </v:var>
                        <v:var name="ResultTransInhibit" type="BOOLEAN">
                          <a:a name="DESC" 
                              value="EN: Result Transfer Inhibit / Decimation Filter Pre-Fill."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                              type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="3cacb124-d0108-480e-9ae9-ad701abac082"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="FALSE"/>
                        </v:var>
                        <v:var name="DestSelect" type="ENUMERATION">
                          <a:a name="DESC"
                            value="EN: Conversion Result Destination Selection."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                              type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                              value="3cacb124-d0108-480e-9ae9-ad701abac083"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="EQADC_ALT_DEST_RFIFO"/>
                          <a:da name="RANGE">
                            <a:v>EQADC_ALT_DEST_RFIFO</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_A</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_B</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_C</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_D</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_E</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_F</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_G</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_A_E</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_B_E</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_C_E</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_D_E</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_A_G</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_B_G</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_C_G</a:v>
                            <a:v>EQADC_ALT_DEST_DECFILT_D_G</a:v>
                          </a:da>
                        </v:var>
                        <v:var name="FMTA_SignEn" type="BOOLEAN">
                          <a:a name="DESC" 
                              value="EN: TRUR:Right justified signed. FALSE:Right justified unsigned."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                              type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="3cacb124-d0109-480e-9ae9-ad701abac090"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="FALSE"/>
                        </v:var>
                        <v:var name="PreGain" type="ENUMERATION">
                          <a:a name="DESC"
                            value="EN: ADC Pre-gain control."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                              type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
