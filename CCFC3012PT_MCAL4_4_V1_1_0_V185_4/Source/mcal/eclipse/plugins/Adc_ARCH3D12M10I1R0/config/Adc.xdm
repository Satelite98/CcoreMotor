                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="f39c116f-78e4-4c92-8782-6dd6a6116e11"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="REF" 
                            value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/eDma/eDmaConfigSet/eDmaChannelConfig"/>
                          <a:a name="EDITABLE" type="XPath">
                              <a:tst expr="../GroupSelIntOrDma = 'DMA'"/>
                          </a:a>
                        </v:ref>

                        <!-- ResulteDmaChannel -->
                        <v:ref name="ResulteDmaChannel" type="REFERENCE">
                          <a:a name="DESC" 
                               value="EN: Reference to the RFIFO dma channel of the edma module."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="f33c956f-78e4-4c92-8952-6da6a6336e42"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="REF" 
                            value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/eDma/eDmaConfigSet/eDmaChannelConfig"/>
                          <a:a name="EDITABLE" type="XPath">
                              <a:tst expr="../GroupSelIntOrDma = 'DMA'"/>
                          </a:a>
                        </v:ref>

                        <!-- AdcHwTrigSrc -->
                        <v:var name="AdcHwTrigSrc" type="ENUMERATION">
                          <a:a name="DESC"
                            value="EN: Selects the source for the eQADC trigger inputs"/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="acd194cf-69d7-4f1d-9db8-e6b85e021d56"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" type="XPath" expr="ecu:list(concat('EQADC.AdcGroup.Fifo',substring-after(string(node:fallback(../AdcGroupCFifo,'EQADC_FIFO_0')), 'EQADC_FIFO_'),'.AdcHwTrigSrc'))[position()-1=node:fallback(node:current()/../@index,0)]"/>
                          <a:da name="RANGE" type="XPath" expr="ecu:list(concat('EQADC.AdcGroup.Fifo',substring-after(string(node:fallback(../AdcGroupCFifo,'')), 'EQADC_FIFO_'),'.AdcHwTrigSrc'))"/>
                          <a:a name="EDITABLE" type="XPath">
                              <a:tst expr="../../../../../../AdcGeneral/AdcHwTriggerApi = 'true' and ../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_HW'"/>
                          </a:a>
                        </v:var>
                        <v:var name="GtmTrigAdcOutsel" type="ENUMERATION">
                          <a:a name="DESC" 
                               value="EN: When enabling GTM to trigger ADC, select the trigger source"/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="dde62024-fdc0-4af6-a0ae-0411c01e9c01"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="GTM_ADC_TRIG_OUTSEL_NO_TRIG"/>
                          <a:da name="RANGE">
                            <a:v>GTM_ADC_TRIG_OUTSEL_NO_TRIG</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_1</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_2</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_3</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_4</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_5</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_6</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_7</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_8</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_9</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_10</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_11</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_12</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_13</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_14</a:v>
                            <a:v>GTM_ADC_TRIG_OUTSEL_15</a:v>
                          </a:da>
                          <a:a name="EDITABLE" type="XPath">
                              <a:tst expr="(text:contains(node:value(../AdcHwTrigSrc),'GTM'))"/>
                          </a:a>
                        </v:var>
                        <v:var name="TrigDelayEn" type="BOOLEAN">
                          <a:a name="DESC" 
                              value="EN: Enable trigger delay"/>
