                          <a:a name="DESC">
                            <a:v><![CDATA[EN:<html><p>Configures on which edge of the hardware trigger signal the driver should react, i.e. start the conversion (only if supported by the ADC hardware).<bc>
                            1.Open 'AdcHwTriggerApi';<br>
                            2.AdcGroupTriggSrc = 'ADC_TRIGG_SRC_HW';
                            </p></html>]]></a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="79c2589c-df80-4fbc-b9fc-7f8fd3ea6b87"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="ADC_HW_TRIG_RISING_EDGE"/>
                          <a:a name="EDITABLE" type="XPath">
                              <a:tst expr="(../../../../../../AdcGeneral/AdcHwTriggerApi = 'true') and (../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_HW')"/>
                          </a:a>
                          <a:da name="RANGE">
                            <a:v>ADC_HW_TRIG_BOTH_EDGES</a:v>
                            <a:v>ADC_HW_TRIG_FALLING_EDGE</a:v>
                            <a:v>ADC_HW_TRIG_RISING_EDGE</a:v>
                          </a:da>
                        </v:var>

                        <!-- AdcHwTrigTimer -->
                        <v:var name="AdcHwTrigTimer" type="INTEGER">
                          <a:a name="DESC" 
                               value="EN: Reload value of the ADC module embedded timer (only if supported by ADC hardware)."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="998ad00b-3e16-4f17-849e-edfe76958d4c"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="READONLY" value="TRUE"/>
                          <a:da name="DEFAULT" value="0"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&lt;=9223372036854775807"/>
                            <a:tst expr="&gt;=0"/>
                          </a:da>
                        </v:var>

                        <!-- AdcNotification -->
                        <v:var name="AdcNotification" type="FUNCTION-NAME">
                          <a:a name="DESC" 
                               value="EN: Callback function for each group"/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="7ef2a73c-2a14-43e3-8502-b64810d0e365"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="NULL_PTR"/>
                          <a:a name="EDITABLE" type="XPath">
                              <a:tst expr="../../../../../../AdcGeneral/AdcGrpNotifCapability = 'true'"/>
                          </a:a>
                        </v:var>

                        <!-- Overflow_Notification -->
                        <v:var name="Overflow_Notification" type="FUNCTION-NAME">
                          <a:a name="DESC" 
                               value="EN: Overflow Callback function for each group.
                                      Note:It can be configured when the AdcGeneral/EnableIntRFOF or AdcGeneral/EnableIntTORF configuration options are checked."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="dde62024-fdc0-4af6-a0ae-0417c01e9c00"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="NULL_PTR"/>
                          <a:a name="EDITABLE" type="XPath">
                            <a:tst expr="../../../../../../AdcGeneral/EnableIntRFOF = 'true' or ../../../../../../AdcGeneral/EnableIntTORF = 'true'"/>
                          </a:a>
                        </v:var>

                        <!-- AdcStreamingBufferMode -->
                        <v:var name="AdcStreamingBufferMode" type="ENUMERATION">
                          <a:a name="DESC" 
                               value="EN: Configure streaming buffer as &quot;linear buffer&quot; (i.e. the ADC Driver stops the conversion as soon as the stream buffer is full) or as &quot;ring buffer&quot; (wraps around if the end of the stream buffer is reached)."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="50d08bec-4cdc-424b-9030-7b71bd18197d"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="ADC_STREAM_BUFFER_LINEAR"/>
                          <a:da name="RANGE">
                            <a:v>ADC_STREAM_BUFFER_CIRCULAR</a:v>
                            <a:v>ADC_STREAM_BUFFER_LINEAR</a:v>
                          </a:da>
                        </v:var>

                        <!-- AdcStreamingNumSamples -->
                        <v:var name="AdcStreamingNumSamples" type="INTEGER">
                          <a:a name="DESC" 
                               value="EN: Number of ADC values to be acquired per channel in streaming access mode.
                                      Note:If GroupSelIntOrDma selects dma, then AdcStreamingNumSamples can be configured only if DmaSampleNum is not equal to 1."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="f39c006f-78e4-4c92-8782-6dd6a6006e42"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="1"/>
                          <a:a name="INVALID" type="XPath">
                              <a:tst expr="((../GroupSelIntOrDma = 'DMA') and (../DmaSampleNum != '1') and (. != '1'))"
                                     true="If GroupSelIntOrDma selects dma, then AdcStreamingNumSamples can be configured only if DmaSampleNum is not equal to 1."/>
                          </a:a>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&lt;=255"/>
                            <a:tst expr="&gt;=1"/>
                          </a:da>
                        </v:var>

                        <!-- AdcGroupCFifo -->
                        <v:var name="AdcGroupCFifo" type="ENUMERATION">
                          <a:a name="DESC">
                            <a:v><![CDATA[EN:<html><p>Select Command FIFO.<br>
                            Each EQADC instance has 2 ADC units and 6 CFIFOs, which means:<br><br>
                            <li>ADC0 and ADC1 share (instance0)EQADC_FIFO_0 to EQADC_FIFO_5.</li><br>
                            ...
                            </p></html>]]></a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="d70e352d-1c65-4913-b110-ada0a58fa896"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="EQADC_FIFO_0"/>
                          <a:da name="RANGE">
                            <a:v>EQADC_FIFO_0</a:v>
                            <a:v>EQADC_FIFO_1</a:v>
                            <a:v>EQADC_FIFO_2</a:v>
                            <a:v>EQADC_FIFO_3</a:v>
                            <a:v>EQADC_FIFO_4</a:v>
                            <a:v>EQADC_FIFO_5</a:v>
                          </a:da>
                        </v:var>

                        <!-- DMACompleteClearERQ -->
                        <v:var name="DMACompleteClearERQ" type="BOOLEAN">
                          <a:a name="DESC" 
                              value="EN: The ERQ clearing switch is sent through the DMA channel."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                              type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="3a24d01d-184b-0171-bc18-011872c70d00"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="false"/>
                          <a:a name="EDITABLE" type="XPath">
                              <a:tst expr="../GroupSelIntOrDma = 'DMA'"/>
                          </a:a>
                          <a:da name="INVALID" type="XPath">
                            <a:tst expr="(.= 'false') and (../GroupSelIntOrDma = 'DMA') and (../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_SW') and (../AdcGroupConversionMode = 'ADC_CONV_MODE_ONESHOT')"
                                     true="When the triggering method is software triggered, the conversion method is DMA conversion, and the conversion mode is oneshot, the ERQ clear switch needs to be turned on"/>
                          </a:da>
                        </v:var>

                        <!-- AdcGroupRFifo -->
                        <v:var name="AdcGroupRFifo" type="ENUMERATION">
                          <a:a name="DESC">
                            <a:v><![CDATA[EN:<html><p>Select Result FIFO.<br>
                            Each EQADC instance has 2 ADC units and 6 RFIFOs, which means:<br><br>
                            <li>ADC0 and ADC1 share (instance0)EQADC_FIFO_0 to EQADC_FIFO_5.</li><br>
                            ...
                            </p></html>]]></a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="dg0e386d-1c65-5412-b165-ada0a58ha557"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="EQADC_FIFO_0"/>
                          <a:da name="RANGE">
                            <a:v>EQADC_FIFO_0</a:v>
                            <a:v>EQADC_FIFO_1</a:v>
                            <a:v>EQADC_FIFO_2</a:v>
                            <a:v>EQADC_FIFO_3</a:v>
                            <a:v>EQADC_FIFO_4</a:v>
                            <a:v>EQADC_FIFO_5</a:v>
                          </a:da>
                        </v:var>

                        <!-- GroupSelIntOrDma -->
                        <v:var name="GroupSelIntOrDma" type="ENUMERATION">
                          <a:a name="DESC"
                            value="EN: Select the conversion method."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="bc0e123d-1c85-7642-b165-ada0a58ha963"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="INTERRUPT"/>
                          <a:da name="RANGE">
                            <a:v>INTERRUPT</a:v>
                            <a:v>DMA</a:v>
                            <a:v>WITHOUT_INTERRUPT</a:v>
                          </a:da>
                          <a:da name="INVALID" type="XPath">
                            <a:tst expr="(.= 'WITHOUT_INTERRUPT') and (../AdcGroupTriggSrc = 'ADC_TRIGG_SRC_HW')"
                                     true="Hardware trigger group does not support no interrupt."/>
                          </a:da>
                        </v:var>

                        <!-- DmaSampleNum -->
                        <v:var name="DmaSampleNum" type="INTEGER">
                          <a:a name="DESC" 
                               value="EN: Used to set the number of dma multiple sampling times, when all the sampling is finished, the interrupt callback is generated.
                                          Note: GroupSelIntOrDma = DMA and AdcStreamingNumSamples = 1"/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="CCFC"/>
                          <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                          <a:a name="SCOPE" value="LOCAL"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="dde62024-fdc0-4af6-a0ae-0411c01e9c00"/>
                          <a:a name="__ORIGIN" value="Elektrobit"/>
                          <a:da name="DEFAULT" value="1"/>
                          <a:a name="EDITABLE" type="XPath">
                              <a:tst expr="../GroupSelIntOrDma = 'DMA' and ../AdcStreamingNumSamples = '1'"/>
                          </a:a>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&lt;=100"/>
                            <a:tst expr="&gt;=1"/>
                          </a:da>
                        </v:var>

                        <!-- eDmaChannel -->
                        <v:ref name="eDmaChannel" type="REFERENCE">
                          <a:a name="DESC" 
                               value="EN: Reference to the CFIFO dma channel of the edma module."/>
